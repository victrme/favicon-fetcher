{
	"name": "@victr/favicon-fetcher",
	"version": "2.1.1",
	"author": "Victor Azevedo",
	"description": "Favicon fetcher finds favicons and flawlessly fetches them fast",
	"license": "ISC",
	"keywords": [
		"favicon"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/victrme/favicon-fetcher"
	},
	"scripts": {
		"build": "tsup",
		"dry-run": "tsup && npm publish --dry-run"
	},
	"devDependencies": {
		"tsup": "^8.3.5",
		"typescript": "^5.7.3"
	},
	"files": [
		"/dist"
	],
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		}
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"format": "esm",
		"dts": true,
		"clean": true
	},
	"type": "module"
}
